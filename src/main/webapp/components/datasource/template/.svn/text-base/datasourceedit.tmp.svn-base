<div id="container">
        <section class="right_content">
            <div class="breadcrumb_div">
                <div class="bread_crumb"><div class="mob_hidden">Home <span>>&nbsp </span>Admin <span>> &nbsp </span></div><span> Data Source</span></div>
            </div>
            <div class="create_div">
                <div class="create_table_div">
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="commonlabel.label.name"></span><sup>*</sup></label>
                        <div class="col-sm-10"><input id="edit_name" class="form-control" type="text" value="{{data.name}}" maxlength="30"></div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="commonlabel.label.description"></span></label>
                        <div class="col-sm-10"><textarea id="editdesc" class="form-control" value="{{data.description}}" maxlength="150">{{data.description}}</textarea></div>
                    </div>            
					<div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="datasource.appId"></span><sup>*</sup></label>
                        <div class="col-sm-10"><input id="edit_appId" class="form-control" type="text" value="{{data.appId}}" maxlength="30"></div>
                    </div> 	
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="commonlabel.label.responseType"></span></label>
                        <div class="col-sm-10">
                        	<select id="edittype" class="form-control">                        		
                            </select>
                        </div>
                    </div> 
					<div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="datasource.storeType"></span></label>
                        <div class="col-sm-10">
                        	<select dynId="{{data.id}}" id="storetype" class="form-control">
                        			<option dynId="{{data.mobileAPPStoreTypeId}}"></option>
							</select>
                        </div>
                    </div>		
                </div>
                <div class="create_buttons">
                    <input id="datasrcupdate" type="button" class="btn_style create_btn" data-i18n="[value]commonlabel.btn.update;">
                    <input id="cancel_update" class="btn_style cancel_icon_btn" type="button" data-i18n="[value]commonlabel.btn.cancel;">
                </div>
            </div>
            <!--main-content div ends -->
        </section>
</div>