<div id="container">
        <section class="right_content">
            <div class="breadcrumb_div">
                <div class="bread_crumb"><div class="mob_hidden">Home <span>>&nbsp </span>Admin <span>> &nbsp </span></div><span> Groups</span></div>
            </div>
            <div class="create_div">
                <div class="create_table_div">
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="commonlabel.label.name"></span></label>
                        <div class="col-sm-10"><input id="groupname" class="form-control" type="text" value="{{data.name}}" maxlength="30"></div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="commonlabel.label.description"></span></label>
                        <div class="col-sm-10"><textarea id="groupdesc" class="form-control" maxlength="150">{{data.description}}</textarea></div>
                    </div>
					<div class="form-group">
						<div class="move_contents col-sm-7">
							<table cellpadding="0" cellspacing="0" border="0" width="100%">
								<tr>
									<td>
										Users:<br/>
										<div class="muliselect_div" id="permissions">
											<ul>
											</ul>
										</div>
									</td>
									<td class="select_leftright">
										<input type='button' id='btnRight' value ='  >  ' class="move_leftright"/>
										<br/><input type='button' id='btnLeft' value ='  <  ' class="move_leftright"/>
									</td>
									<td>
										Assigned: <br/>
										<div class="muliselect_div" id="assigned">
											<ul>
											</ul>
										</div>
									</td>
								</tr>
							</table>
						</div>
					</div>
					
					<div class="form-group">
						<div class="move_contents col-sm-7">
							<table cellpadding="0" cellspacing="0" border="0" width="100%">
								<tr>
									<td>
										Roles:<br/>
										<div class="muliselect_div" id="rolepermissions">
											<ul>
											</ul>
										</div>
									</td>
									<td class="select_leftright">
										<input type='button' id='btnRightrole' value ='  >  ' class="move_leftright"/>
										<br/><input type='button' id='btnLeftrole' value ='  <  ' class="move_leftright"/>
									</td>
									<td>
										Assigned: <br/>
										<div class="muliselect_div" id="rolesassigned">
											<ul>
											</ul>
										</div>
									</td>
								</tr>
							</table>
						</div>
					</div>
                </div>
                <div class="create_buttons">
                    <input id="groupsupdate" type="button" class="btn_style create_btn" data-i18n="[value]commonlabel.btn.update;">
                    <input id="cancel_group" class="btn_style cancel_icon_btn" type="button" data-i18n="[value]commonlabel.btn.cancel;">
                </div>
            </div>
            <!--main-content div ends -->
        </section>
</div>