<div id="container">
        <section class="right_content">
            <div class="breadcrumb_div">
                <div class="bread_crumb"><div class="mob_hidden">Home <span>>&nbsp </span>Admin <span>> &nbsp </span></div><span> User</span></div>
            </div>
            <div class="create_div">
                <div class="create_table_div">
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="user.firstname"></span><sup>*</sup></label>
                        <div class="col-sm-10"><input id="userfirstname" class="form-control" type="text" maxlength="30"></div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="user.lastname"></span><sup>*</sup></label>
                        <div class="col-sm-10"><input id="userlastname" class="form-control" maxlength="30"></textarea></div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="user.email"></span><sup>*</sup></label>
                        <div class="col-sm-10"><input id="useremail" class="form-control" type="text" maxlength="30"><span id="emailError"></span></div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="user.password"></span><sup>*</sup></label>
                        <div class="col-sm-10"><input id="password" class="form-control" type="password" maxlength="30"><span id="pwdError"></span></div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="user.confirmpwd"></span><sup>*</sup></label>
                        <div class="col-sm-10"><input id="confirmpwd" class="form-control" type="password" maxlength="30"><span id="confirmpwdError"></span></div>
                    </div>
                    <div class="form-group">
						<div class="move_contents col-sm-7">
							<table cellpadding="0" cellspacing="0" border="0" width="100%">
								<tr>
									<td>
										Roles:<br/>
										<select multiple="multiple" id='Roles'>
										</select>
									</td>
									<td class="select_leftright">
										<input type='button' id='btnRight' value ='  >  ' class="move_leftright"/>
										<br/><input type='button' id='btnLeft' value ='  <  ' class="move_leftright"/>
									</td>
									<td>
										Assigned: <br/>
										<select multiple="multiple" id='assigned'>
										</select>
									</td>
								</tr>
							</table>
						</div>
					</div>
                </div>
                <div class="create_buttons">
                    <input id="usercreate" type="button" class="btn_style create_btn" data-i18n="[value]commonlabel.btn.create;">
                    <input id="cancel_user" class="btn_style cancel_icon_btn" type="button" data-i18n="[value]commonlabel.btn.cancel;">
                </div>
            </div>
            <!--main-content div ends -->
        </section>
</div>