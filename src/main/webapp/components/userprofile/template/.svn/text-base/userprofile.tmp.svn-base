<div id="container">
        <section class="right_content">
            <div class="breadcrumb_div">
                <div class="bread_crumb">Home</div>
            </div>
            <div class="create_div">
                <div class="create_table_div">
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="userProfile.firstname"></span><sup>*</sup></label>
                        <div class="col-sm-10"><input id="userfirstname" class="form-control" type="text" value="{{userlist.firstName}}" maxlength="30"></div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="userProfile.lastname"></span><sup>*</sup></label>
                        <div class="col-sm-10"><input id="userlastname" class="form-control" value="{{userlist.lastName}}" maxlength="30"></textarea></div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="userProfile.email"></span><sup>*</sup></label>
                        <div class="col-sm-10"><input id="useremail" class="form-control" type="text" value="{{userlist.emailId}}" maxlength="30"><!--<span id="emailError">--></span></div>
                    </div>
					 <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="userProfile.changeprofilepicture"></span></label>
                        <div class="col-sm-4">
							<input id="profilePicture" class="form-control select_picture" value="{{userlist.profilePicture}}" type="text" maxlength="30">
						</div>
						
							<div id="uploadImage" class="uploadImage-file-uploader">
								<div class="qq-uploader">
									<div class="qq-upload-button btn btn_style" name="qq-upload-file" style="position: relative; overflow: hidden; direction: ltr;">

									</div>
									<ul class="qq-upload-list"></ul>
								</div>
							</div>
						
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="userProfile.oldpassword"></span></label>
                        <div class="col-sm-10"><input id="oldpassword" class="form-control" type="text" maxlength="30"></div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="userProfile.newpassword"></span></label>
                        <div class="col-sm-10"><input id="password" class="form-control" type="text" maxlength="30"></div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label"><span data-i18n="userProfile.confirmpassword"></span></label>
                        <div class="col-sm-10"><input id="confirmpwd" class="form-control" type="text" maxlength="30"><!--<span id="confirmPwdError">--></span></div>
                    </div>
                </div>
                <div class="create_buttons">
                    <input id="usercreate" type="button" class="btn_style create_btn" data-i18n="[value]commonlabel.btn.update;">
                    <input id="cancel_user" class="btn_style cancel_icon_btn" type="button" data-i18n="[value]commonlabel.btn.cancel;">
                </div>
            </div>
            <!--main-content div ends -->
        </section>
</div>